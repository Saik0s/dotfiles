function rr {
    echo "Removing $1"
    rm -rf $1
}
function rpref {
    rr ~/Library/Preferences/$1
}
function rcach {
    rr ~/Library/Caches/$1
}
function rcook {
    rr ~/Library/Cookies/$1
}
rpref com.surteesstudios.Bartender.plist
rcach com.surteesstudios.Bartender
rcook com.surteesstudios.Bartender.binarycookies

# vim: set filetype=zsh :
